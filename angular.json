{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "components-docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/components-docs/",
      "sourceRoot": "projects/components-docs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist-docs",
            "index": "projects/components-docs/src/index.html",
            "main": "projects/components-docs/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/components-docs/tsconfig.app.json",
            "assets": [
              "projects/components-docs/src/favicon.ico",
              "projects/components-docs/src/assets"
            ],
            "styles": ["projects/components-docs/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "components-docs:build:production"
            },
            "development": {
              "browserTarget": "components-docs:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
    "components-lib": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/components-lib/",
      "sourceRoot": "projects/components-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/components-lib/tsconfig.lib.json",
            "project": "projects/components-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/components-lib/ng-package.prod.json",
              "tsConfig": "projects/components-lib/tsconfig.lib.prod.json"
            }
          }
        }
      }
    },
    "schems": {
      "projectType": "library",
      "root": "projects/schems",
      "sourceRoot": "projects/schems/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/schems/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/schems/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/schems/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/schems/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "my-first-app": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/my-first-app",
      "sourceRoot": "projects/my-first-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/my-first-app",
            "index": "projects/my-first-app/src/index.html",
            "main": "projects/my-first-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/my-first-app/tsconfig.app.json",
            "assets": [
              "projects/my-first-app/src/favicon.ico",
              "projects/my-first-app/src/assets"
            ],
            "styles": [
              "projects/my-first-app/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "my-first-app:build:production"
            },
            "development": {
              "browserTarget": "my-first-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "my-first-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/my-first-app/tsconfig.spec.json",
            "assets": [
              "projects/my-first-app/src/favicon.ico",
              "projects/my-first-app/src/assets"
            ],
            "styles": [
              "projects/my-first-app/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
